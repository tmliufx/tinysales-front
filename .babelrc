{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true
    }],
    "add-module-exports",
    "transform-es2015-destructuring",
    "transform-object-rest-spread",
    "transform-async-to-generator"
  ]
}