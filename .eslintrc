{
  "extends": "airbnb",
  "globals": {
    "fetch": false,
    "FormData": false
  },
  "rules": {
    "comma-dangle": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": false }],
    "global-require": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "no-case-declarations": 0,
    "no-await-in-loop": 0,
    "no-restricted-syntax": 0
  }
}